{
  "name": "project-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/src/index.js",
    "dev": "cp ./src/environments/.env.dev ./.env && cp Procfile.dev Procfile && nodemon ./src/index.ts",
    "staging": "cp ./src/environments/.env.staging ./.env && cp Procfile.staging Procfile && ts-node ./src/index.ts",
    "prod": "cp ./src/environments/.env.prod ./.env && cp Procfile.prod Procfile tsc --build tsconfig.prod.json && node ./dist/src/index.js",
    "wdev": "copy .\\src\\environments\\.env.dev .\\.env && copy Procfile.dev Procfile && nodemon .\\src\\index.ts",
    "wstaging": "copy .\\src\\environments\\.env.staging .\\.env && copy Procfile.staging Procfile && ts-node .\\src\\index.ts",
    "wprod": "copy .\\src\\environments\\.env.prod .\\.env &&  && copy Procfile.prod Procfile && tsc --build tsconfig.prod.json  node .\\dist\\src\\index.js",
    "docker-dev-build": "cp ./src/environments/docker/Dockerfile.dev ./Dockerfile && docker build -t backend-redil .",
    "docker-dev-run": "cp ./src/environments/docker/Dockerfile.dev ./Dockerfile && docker run -p 3000:3000 backend-redil",
    "docker-prod-build": "cp ./src/environments/docker/Dockerfile.prod ./Dockerfile && docker build -t backend-redil .",
    "docker-prod-run": "cp ./src/environments/docker/Dockerfile.prod ./Dockerfile && docker run -p 5000:5000 backend-redil",
    "docker-staging-build": "cp ./src/environments/docker/Dockerfile.staging ./Dockerfile && docker build -t backend-redil .",
    "docker-staging-run": "cp ./src/environments/docker/Dockerfile.staging ./Dockerfile && docker run -p 8888:8888 backend-redil",
    "docker-stop-all": "docker stop $(docker ps -a -q)"
  },
  "author": "squaads",
  "license": "",
  "dependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/cheerio": "^0.22.20",
    "@types/cors": "^2.8.7",
    "@types/cron": "^1.7.2",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.17.2",
    "@types/mongoose": "^5.10.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.0.10",
    "axios": "^0.19.2",
    "body-parser": "^1.18.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-status-monitor": "^1.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.13",
    "morgan": "^1.10.0",
    "pretty-error": "^2.1.1",
    "swagger-ui": "^3.36.2",
    "swagger-ui-dist": "^3.25.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/cron": "^1.7.2",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "nodemon": "^1.18.3",
    "typescript": "^3.7.5"
  }
}
